#	$(CXX) -c $(INCLUDE) $(OPTIONS) main.cxx


	$(CXX) $(INCLUDE) $(OPTIONS) ptypes.lib WS2_32.Lib User32.Lib get_opt.cxx mem_mngr.cxx main.cxx -o bts_proxy_mux.exe
